<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ap2.MainMenu">
   <children>
      <Button fx:id="exit" layoutX="175.0" layoutY="600.0" mnemonicParsing="false" onAction="#exit" prefHeight="125.0" prefWidth="250.0" text="Exit" textFill="#c42108">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
      <Button fx:id="setting" layoutX="175.0" layoutY="450.0" mnemonicParsing="false" onAction="#setting" prefHeight="125.0" prefWidth="250.0" text="Setting" textFill="#c20aae">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
      <Button layoutX="175.0" layoutY="150.0" mnemonicParsing="false" onAction="#gameMenu" prefHeight="125.0" prefWidth="250.0" text="New Game" textFill="#110ac2">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button layoutX="175.0" layoutY="300.0" mnemonicParsing="false" onAction="#history" prefHeight="125.0" prefWidth="250.0" text="History" textFill="#0ac22e">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="145.0" prefHeight="80.0" prefWidth="309.0" text="Bricks Breaker +" textFill="#c1d921">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="score" alignment="CENTER" layoutX="200.0" layoutY="80.0" prefHeight="50.0" prefWidth="200.0" text="0">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
