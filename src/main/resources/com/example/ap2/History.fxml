<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="750.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ap2.History">
   <children>
      <TableView fx:id="table" layoutX="150.0" prefHeight="600.0" prefWidth="300.0">
        <columns>
          <TableColumn fx:id="nameTable" prefWidth="150.0" text="Name" />
          <TableColumn fx:id="scoreTable" minWidth="50.0" prefWidth="150.0" text="Score" />
        </columns>
      </TableView>
      <Button layoutX="175.0" layoutY="625.0" mnemonicParsing="false" onAction="#back" prefHeight="110.0" prefWidth="250.0" text="Back" textFill="#1c0294">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
